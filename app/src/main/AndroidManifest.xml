        <activity android:name=".Golden_Six_Home"/>
        <activity android:name=".Golden_Six_Input"/>
        <activity android:name=".Golden_Six_Do_Squ"/>
        <activity android:name=".Golden_Six_Do_Ben"/>
        <activity android:name=".Golden_Six_Do_Curl"/>
        <activity android:name=".Golden_Six_Do_Chin"/>
