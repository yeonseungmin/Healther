<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/golden_six_ordinary"
        android:layout_width="336dp"
        android:layout_height="154dp"
        android:layout_marginBottom="36dp"
        android:autoText="false"
        android:text="골든 식스"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/program_ordinary2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/program_ordinary2"
        android:layout_width="336dp"
        android:layout_height="154dp"
        android:layout_marginBottom="36dp"
        android:text="프로그램2"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/program_ordinary3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/program_ordinary3"
        android:layout_width="336dp"
        android:layout_height="154dp"
        android:layout_marginBottom="36dp"
        android:text="프로그램3"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/ordinary_program_banner"
        android:layout_width="411dp"
        android:layout_height="104dp"
        android:gravity="center"
        android:text="일반인용"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/golden_six_ordinary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>